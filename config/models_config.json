{
  "models": {
    "MRT-OAST": {
      "path": "repos/MRT-OAST",
      "train_script": "./train.sh",
      "models": ["default"],
      "supported_hyperparams": {
        "epochs": {
          "flag": "--epochs",
          "type": "int",
          "default": 10,
          "range": [5, 20]
        },
        "learning_rate": {
          "flag": "--lr",
          "type": "float",
          "default": 0.0001,
          "range": [0.00001, 0.001]
        },
        "seed": {
          "flag": "--seed",
          "type": "int",
          "default": 1334,
          "range": [0, 9999]
        },
        "dropout": {
          "flag": "--dropout",
          "type": "float",
          "default": 0.2,
          "range": [0.0, 0.5]
        },
        "weight_decay": {
          "flag": "--weight-decay",
          "type": "float",
          "default": 0.0,
          "range": [0.0, 0.01]
        }
      },
      "performance_metrics": {
        "log_patterns": {
          "accuracy": "Accuracy[:\\s]+([0-9.]+)",
          "precision": "Precision[:\\s]+([0-9.]+)",
          "recall": "Recall[:\\s]+([0-9.]+)",
          "f1": "F1[:\\s]+([0-9.]+)"
        }
      }
    },
