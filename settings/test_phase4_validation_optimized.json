        "model": "mlp",
        "mutation_type": "mutation",
        "mutate_params": {
          "learning_rate": {
            "distribution": "log_uniform",
            "min": 1e-5,
            "max": 1e-3
          }
        }
      },
      "background": {
        "repo": "examples",
