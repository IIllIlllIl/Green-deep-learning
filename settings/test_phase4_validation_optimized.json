{
  "comment": "Phase 4 验证配置 - 精简版（12-18小时目标）",
  "description": "测试修复后的正则表达式，优先恢复最重要的实验",
  "strategy": "每个模型-模式-参数组合运行2-3个实验（而非全部5个）",
  "estimated_time_without_dedup": "20.8小时",
  "estimated_time_with_50pct_dedup": "10.4小时",
  "estimated_time_with_70pct_dedup": "6.2小时",
  "target_experiments": 42,

  "global_settings": {
    "runs_per_config": 1,
    "use_deduplication": true,
    "historical_csvs": ["results/raw_data.csv"]
  },

  "experiments": [
    {
      "comment": "VulBERTa/mlp - 非并行模式 - 默认值实验（优先级最高）",
      "repo": "VulBERTa",
      "model": "mlp",
      "runs_per_config": 3,
      "mutation_type": "default"
    },
    {
      "comment": "VulBERTa/mlp - 非并行模式 - epochs变异",
      "repo": "VulBERTa",
      "model": "mlp",
      "runs_per_config": 2,
      "mutation_type": "mutation",
      "mutate_params": {
        "epochs": {
          "distribution": "log_uniform",
          "min": 5,
          "max": 20
        }
      }
    },
    {
      "comment": "VulBERTa/mlp - 非并行模式 - learning_rate变异",
      "repo": "VulBERTa",
      "model": "mlp",
      "runs_per_config": 2,
      "mutation_type": "mutation",
      "mutate_params": {
        "learning_rate": {
          "distribution": "log_uniform",
          "min": 1e-5,
          "max": 1e-3
        }
      }
    },
    {
      "comment": "VulBERTa/mlp - 非并行模式 - weight_decay变异",
      "repo": "VulBERTa",
      "model": "mlp",
      "runs_per_config": 2,
      "mutation_type": "mutation",
      "mutate_params": {
        "weight_decay": {
          "distribution": "log_uniform",
          "min": 1e-5,
          "max": 1e-1
        }
      }
    },
    {
      "comment": "VulBERTa/mlp - 非并行模式 - seed变异",
      "repo": "VulBERTa",
      "model": "mlp",
      "runs_per_config": 2,
      "mutation_type": "mutation",
      "mutate_params": {
        "seed": {
          "distribution": "uniform",
          "min": 1,
          "max": 10000
        }
      }
    },
    {
      "comment": "VulBERTa/mlp - 并行模式 - 默认值实验（验证并行模式性能提取）",
      "mutation_type": "parallel",
      "runs_per_config": 1,
      "foreground": {
        "repo": "VulBERTa",
        "model": "mlp",
        "mutation_type": "default"
      },
      "background": {
        "repo": "examples",
        "model": "mnist"
      }
    },
    {
      "comment": "VulBERTa/mlp - 并行模式 - learning_rate变异（最重要参数）",
