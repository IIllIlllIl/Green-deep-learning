{
  "experiment_name": "single_repo_all_models",
  "description": "Mutate all hyperparameters for all models in pytorch_resnet_cifar10",
  "governor": "performance",
  "runs_per_config": 3,
  "max_retries": 2,
  "experiments": [
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet20",
      "mutate": ["all"]
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet32",
      "mutate": ["all"]
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet44",
      "mutate": ["all"]
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet56",
      "mutate": ["all"]
    }
  ]
}
