{
  "experiment_name": "mixed_mode_demo",
  "description": "Demonstration of mixed mode - some models with default params, some with mutations",
  "governor": "performance",
  "runs_per_config": 1,
  "max_retries": 2,
  "experiments": [
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet20",
      "mode": "default",
      "hyperparameters": {
        "epochs": 200,
        "learning_rate": 0.1,
        "weight_decay": 0.0001
      },
      "comment": "Baseline: default hyperparameters"
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet20",
      "mode": "mutation",
      "mutate": ["learning_rate"],
      "comment": "Experiment: mutate learning rate only"
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet20",
      "mode": "mutation",
      "mutate": ["all"],
      "comment": "Experiment: mutate all hyperparameters"
    }
  ]
}
