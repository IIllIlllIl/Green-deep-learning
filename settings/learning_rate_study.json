{
  "experiment_name": "learning_rate_study",
  "description": "Study the impact of learning rate on energy consumption and performance",
  "governor": "performance",
  "runs_per_config": 10,
  "max_retries": 2,
  "experiments": [
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet20",
      "mutate": ["learning_rate"],
      "comment": "Only mutate learning rate, keep other params at defaults"
    },
    {
      "repo": "VulBERTa",
      "model": "mlp",
      "mutate": ["learning_rate"]
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "densenet121",
      "mutate": ["learning_rate"]
    }
  ]
}
