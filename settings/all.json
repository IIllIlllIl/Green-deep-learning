{
  "experiment_name": "all_mutations",
  "description": "Mutate all supported hyperparameters for all models across all repositories",
  "governor": "performance",
  "runs_per_config": 5,
  "max_retries": 2,
  "experiments": [
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet20",
      "mutate": ["all"],
      "comment": "变异所有支持的超参数：epochs, learning_rate, seed, weight_decay"
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet32",
      "mutate": ["all"]
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet44",
      "mutate": ["all"]
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet56",
      "mutate": ["all"]
    },
    {
      "repo": "VulBERTa",
      "model": "mlp",
      "mutate": ["all"],
      "comment": "变异所有支持的超参数：epochs, learning_rate, seed, weight_decay"
    },
    {
      "repo": "VulBERTa",
      "model": "cnn",
      "mutate": ["all"]
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "densenet121",
      "mutate": ["all"],
      "comment": "变异所有支持的超参数：epochs, learning_rate, seed, dropout"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "hrnet18",
      "mutate": ["all"]
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mutate": ["all"]
    },
    {
      "repo": "MRT-OAST",
      "model": "default",
      "mutate": ["all"],
      "comment": "变异所有支持的超参数：epochs, learning_rate, seed, dropout, weight_decay"
    },
    {
      "repo": "bug-localization-by-dnn-and-rvsm",
      "model": "default",
      "mutate": ["all"],
      "comment": "变异所有支持的超参数：epochs, learning_rate, seed"
    },
    {
      "repo": "examples",
      "model": "mnist_cnn",
      "mutate": ["all"],
      "comment": "变异所有支持的超参数：epochs, learning_rate, seed"
