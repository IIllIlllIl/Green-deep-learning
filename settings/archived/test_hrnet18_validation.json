{
  "experiment_name": "hrnet18_validation_test",
  "description": "Validate hrnet18 in both sequential and parallel modes with offline setup",
  "governor": "performance",
  "runs_per_config": 1,
  "max_retries": 2,
  "experiments": [
    {
      "mode": "default",
      "repo": "Person_reID_baseline_pytorch",
      "model": "hrnet18",
      "hyperparameters": {
        "epochs": 5,
        "batch_size": 24,
        "learning_rate": 0.05,
        "dropout": 0.5,
        "seed": 1334
      }
    },
    {
      "mode": "parallel",
      "foreground": {
        "repo": "Person_reID_baseline_pytorch",
        "model": "hrnet18",
        "mode": "default",
        "hyperparameters": {
          "epochs": 5,
          "batch_size": 24,
          "learning_rate": 0.05,
          "dropout": 0.5,
          "seed": 1334
        }
      },
      "background": {
        "repo": "examples",
        "model": "mnist_ff",
        "hyperparameters": {
          "epochs": 10,
          "batch_size": 32,
          "learning_rate": 0.01,
          "seed": 1
        }
      }
    }
  ]
}
