    {
      "repo": "VulBERTa",
      "model": "cnn",
      "mode": "default",
      "comment": "VulBERTa-CNN: 默认值实验（2次）"
    },
    {
      "repo": "VulBERTa",
      "model": "cnn",
      "num_mutations": 5,
      "mutate": [
        "epochs"
      ],
      "comment": "VulBERTa-CNN: epochs需要5个新值（10次实验）"
    },
    {
      "repo": "VulBERTa",
      "model": "cnn",
      "num_mutations": 5,
      "mutate": [
        "learning_rate"
      ],
      "comment": "VulBERTa-CNN: learning_rate需要5个新值（10次实验）"
    },
