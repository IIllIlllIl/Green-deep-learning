{
  "experiment_name": "mutation_2x_supplement_complete",
  "mode": "batch",
  "runs_per_config": 2,
  "max_retries": 2,
  "governor": "performance",
  "cleanup_gpu_memory": true,
  "cleanup_between_experiments": true,
  "use_deduplication": true,
  "historical_csvs": [
    "results/summary_all.csv"
  ],
