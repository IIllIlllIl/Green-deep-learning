{
  "experiment_name": "stage7_pcb_parallel",
  "description": "阶段7: Person_reID_pcb模型的并行实验 (预计40小时)",
  "mode": "mutation",
  "runs_per_config": 6,
  "max_retries": 2,
  "governor": "performance",
  "use_deduplication": true,
  "historical_csvs": [
    "results/summary_all.csv"
  ],
  "experiments": [
    {
      "comment": "=== Person_reID_pcb 并行: 20个实验 (预计40小时) ===",
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "parallel",
      "foreground": {
        "repo": "Person_reID_baseline_pytorch",
        "model": "pcb",
        "mode": "mutation",
        "mutate": ["epochs"]
      },
      "background": {
        "repo": "examples",
        "model": "mnist",
        "hyperparameters": {}
      },
      "comment": "epochs需要5个唯一值,运行5次"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "parallel",
      "foreground": {
        "repo": "Person_reID_baseline_pytorch",
        "model": "pcb",
        "mode": "mutation",
        "mutate": ["learning_rate"]
      },
      "background": {
        "repo": "examples",
        "model": "mnist",
        "hyperparameters": {}
      },
      "comment": "learning_rate需要5个唯一值,运行5次"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "parallel",
      "foreground": {
        "repo": "Person_reID_baseline_pytorch",
        "model": "pcb",
        "mode": "mutation",
        "mutate": ["dropout"]
      },
      "background": {
        "repo": "examples",
        "model": "mnist",
        "hyperparameters": {}
      },
      "comment": "dropout需要5个唯一值,运行5次"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "parallel",
      "foreground": {
        "repo": "Person_reID_baseline_pytorch",
        "model": "pcb",
        "mode": "mutation",
        "mutate": ["seed"]
      },
      "background": {
        "repo": "examples",
        "model": "mnist",
        "hyperparameters": {}
      },
      "comment": "seed需要5个唯一值,运行5次"
    }
  ]
}
