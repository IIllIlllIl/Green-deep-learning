{
  "experiment_name": "person_reid_dropout_adjusted_boundary_test",
  "description": "验证Person_reID模型dropout范围[0.3, 0.6]（d-0.2到d+0.1）的性能影响。测试边界值0.3(下界), 0.5(默认), 0.6(上界)，每个配置运行1次。",
  "governor": "performance",
  "runs_per_config": 1,
  "max_retries": 2,
  "mode": "default",
  "experiments": [
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "densenet121",
      "mode": "default",
      "hyperparameters": {
        "epochs": 60,
        "learning_rate": 0.05,
        "dropout": 0.6
      },
      "note": "densenet121 - dropout=0.6 (上边界, d+0.1)"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "hrnet18",
      "mode": "default",
      "hyperparameters": {
        "epochs": 60,
        "learning_rate": 0.05,
        "dropout": 0.6
      },
      "note": "hrnet18 - dropout=0.6 (上边界, d+0.1)"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "default",
      "hyperparameters": {
        "epochs": 60,
        "learning_rate": 0.05,
        "dropout": 0.6
      },
      "note": "pcb - dropout=0.6 (上边界, d+0.1)"
    }
  ]
}
