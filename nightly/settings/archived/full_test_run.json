{
  "experiment_name": "full_test_run",
  "description": "Complete test run - train one model from each repository to validate scripts functionality and robustness",
  "governor": "performance",
  "runs_per_config": 1,
  "max_retries": 2,
  "mode": "default",
  "experiments": [
    {
      "repo": "examples",
      "model": "mnist",
      "mode": "default",
      "hyperparameters": {
        "epochs": 10,
        "learning_rate": 0.01,
        "batch_size": 32,
        "seed": 1
      },
      "comment": "最快的模型 - 用于初步验证"
    },
    {
      "repo": "MRT-OAST",
      "model": "default",
      "mode": "default",
      "hyperparameters": {
        "epochs": 10,
        "learning_rate": 0.0001,
        "seed": 1334,
        "dropout": 0.2,
        "weight_decay": 0.0
      },
      "comment": "MRT-OAST默认配置"
    },
    {
      "repo": "bug-localization-by-dnn-and-rvsm",
      "model": "default",
      "mode": "default",
      "hyperparameters": {
        "max_iter": 10000,
        "kfold": 10,
        "alpha": 0.00001,
        "seed": 42
      },
      "comment": "Bug定位默认配置（使用正确的参数名称）"
    },
    {
      "repo": "VulBERTa",
      "model": "mlp",
      "mode": "default",
      "hyperparameters": {
        "epochs": 10,
        "learning_rate": 0.00003,
        "seed": 42,
        "weight_decay": 0.0
      },
      "comment": "VulBERTa MLP（比CNN快）"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "densenet121",
      "mode": "default",
      "hyperparameters": {
        "epochs": 60,
        "learning_rate": 0.05,
        "dropout": 0.5
      },
      "comment": "DenseNet121默认配置（训练时间较长）"
    },
    {
      "repo": "pytorch_resnet_cifar10",
      "model": "resnet20",
      "mode": "default",
      "hyperparameters": {
        "epochs": 200,
        "learning_rate": 0.1,
        "weight_decay": 0.0001
      },
      "comment": "ResNet20最小模型（训练时间最长，放最后）"
    }
  ]
}
