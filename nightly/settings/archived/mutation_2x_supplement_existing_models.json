{
  "experiment_name": "mutation_2x_supplement_existing_models",
  "mode": "batch",
  "runs_per_config": 2,
  "max_retries": 2,
  "governor": "performance",
  "cleanup_gpu_memory": true,
  "cleanup_between_experiments": true,
  "use_deduplication": true,
  "historical_csvs": [
    "results/summary_all.csv"
  ],
  "experiments": [
    {
      "repo": "bug-localization-by-dnn-and-rvsm",
      "model": "default",
      "num_mutations": 2,
      "mutate": [
        "kfold"
      ],
      "comment": "bug-localization: kfold还需2个新值（4次实验）"
    },
    {
      "repo": "examples",
      "model": "mnist",
      "num_mutations": 2,
      "mutate": [
        "batch_size"
      ],
      "comment": "MNIST: batch_size还需2个新值（4次实验）"
    },
    {
      "repo": "examples",
      "model": "mnist_rnn",
      "num_mutations": 2,
      "mutate": [
        "batch_size"
      ],
      "comment": "MNIST-RNN: batch_size还需2个新值（4次实验）"
    },
    {
      "repo": "examples",
      "model": "siamese",
      "num_mutations": 2,
      "mutate": [
        "batch_size"
      ],
      "comment": "Siamese: batch_size还需2个新值（4次实验）"
    },
    {
      "repo": "examples",
      "model": "mnist_ff",
      "mode": "default",
      "fixed_hyperparams": {
        "batch_size": 10000
      },
      "comment": "MNIST-FF: 默认值实验（2次）"
    },
    {
      "repo": "examples",
      "model": "mnist_ff",
      "num_mutations": 4,
      "mutate": [
        "batch_size"
      ],
      "comment": "MNIST-FF: batch_size还需4个新值（8次实验）"
    },
    {
      "repo": "examples",
      "model": "mnist_ff",
      "num_mutations": 3,
      "mutate": [
        "epochs"
      ],
      "fixed_hyperparams": {
        "batch_size": 10000
      },
      "comment": "MNIST-FF: epochs还需3个新值（6次实验）"
    },
    {
      "repo": "examples",
      "model": "mnist_ff",
      "num_mutations": 3,
      "mutate": [
        "learning_rate"
      ],
      "fixed_hyperparams": {
        "batch_size": 10000
      },
      "comment": "MNIST-FF: learning_rate还需3个新值（6次实验）"
    },
    {
      "repo": "examples",
      "model": "mnist_ff",
      "num_mutations": 3,
      "mutate": [
        "seed"
      ],
      "fixed_hyperparams": {
        "batch_size": 10000
      },
      "comment": "MNIST-FF: seed还需3个新值（6次实验）"
    }
  ]
}