{
  "experiment_name": "11_models_parallel_default_training",
  "description": "11 model parallel training with default epochs - each model appears as foreground exactly once",
  "governor": "performance",
  "runs_per_config": 1,
  "max_retries": 2,
  "mode": "parallel",
  "experiments": [
    {
      "mode": "parallel",
      "foreground": {
        "repo": "pytorch_resnet_cifar10",
        "model": "resnet20",
        "mode": "default",
        "hyperparameters": {
          "epochs": 200,
          "learning_rate": 0.1,
          "weight_decay": 0.0001,
          "seed": 1334
        }
      },
      "background": {
        "repo": "examples",
        "model": "mnist_ff",
        "hyperparameters": {
          "epochs": 10,
          "learning_rate": 0.01,
          "batch_size": 32,
          "seed": 1334
        }
      },
      "note": "Group 1: 超低显存 (1300MB) - resnet20 (default: 200 epochs) + mnist_ff (default: 10 epochs)"
    },
    {
      "mode": "parallel",
      "foreground": {
        "repo": "VulBERTa",
        "model": "mlp",
        "mode": "default",
