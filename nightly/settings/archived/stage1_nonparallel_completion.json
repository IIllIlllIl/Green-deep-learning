{
  "experiment_name": "stage1_nonparallel_completion",
  "description": "阶段1: 完成所有非并行模式的缺口 (预计30小时)",
  "mode": "mutation",
  "runs_per_config": 1,
  "max_retries": 2,
  "governor": "performance",
  "use_deduplication": true,
  "historical_csvs": [
    "results/summary_all.csv"
  ],
  "experiments": [
    {
      "comment": "=== MRT-OAST: 1个dropout值 (预计20分钟) ===",
      "repo": "MRT-OAST",
      "model": "default",
      "mode": "mutation",
      "mutate": ["dropout"]
    },
    {
      "comment": "=== Person_reID_hrnet18: 3个值 (预计3小时) ===",
      "repo": "Person_reID_baseline_pytorch",
      "model": "hrnet18",
      "mode": "mutation",
      "mutate": ["learning_rate"]
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "hrnet18",
      "mode": "mutation",
      "mutate": ["dropout"]
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "hrnet18",
      "mode": "mutation",
      "mutate": ["seed"]
    },
    {
      "comment": "=== Person_reID_pcb: 20个值 (预计23小时) ===",
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "mutation",
      "mutate": ["epochs"],
      "comment": "需要运行5次以生成5个不同的epochs值"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "mutation",
      "mutate": ["learning_rate"],
      "comment": "需要运行5次以生成5个不同的learning_rate值"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "mutation",
      "mutate": ["dropout"],
      "comment": "需要运行5次以生成5个不同的dropout值"
    },
    {
      "repo": "Person_reID_baseline_pytorch",
      "model": "pcb",
      "mode": "mutation",
      "mutate": ["seed"],
      "comment": "需要运行5次以生成5个不同的seed值"
    },
    {
      "comment": "=== mnist_ff: 20个值 (预计3.3小时) ===",
      "repo": "examples",
      "model": "mnist_ff",
      "mode": "mutation",
      "mutate": ["batch_size"],
      "comment": "需要运行5次以生成5个不同的batch_size值"
    },
    {
      "repo": "examples",
      "model": "mnist_ff",
      "mode": "mutation",
      "mutate": ["epochs"],
      "comment": "需要运行5次以生成5个不同的epochs值"
    },
    {
      "repo": "examples",
      "model": "mnist_ff",
      "mode": "mutation",
      "mutate": ["learning_rate"],
      "comment": "需要运行5次以生成5个不同的learning_rate值"
    },
    {
      "repo": "examples",
      "model": "mnist_ff",
      "mode": "mutation",
      "mutate": ["seed"],
      "comment": "需要运行5次以生成5个不同的seed值"
    }
  ]
}
